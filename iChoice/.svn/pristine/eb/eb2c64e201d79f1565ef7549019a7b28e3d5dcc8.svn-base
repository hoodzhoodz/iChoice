<script type="text/javascript">
    function showTab(num) {
        if (num == 1) {
            document.getElementById("Tab_AboutChoiceMMed").style.display = "block";
            document.getElementById("Tab_AboutChoice").style.display = "none";
            addClass(document.getElementById("btn1"), "active");
            removeClass(document.getElementById("btn2"), "active");
        } else {

            document.getElementById("Tab_AboutChoiceMMed").style.display = "none";
            document.getElementById("Tab_AboutChoice").style.display = "block";
            addClass(document.getElementById("btn2"), "active");
            removeClass(document.getElementById("btn1"), "active");
             window.webkit.messageHandlers.showMessage1.postMessage(null);
        }
    }


    function hasClass(ele, cls) {
        var reg = new RegExp('(\\s|^)' + cls + '(\\s|$)', 'ig');
            var match =reg.test( ele.className );
            return match;
        }

    function addClass(ele, cls) {
        if (!this.hasClass(ele, cls))
            ele.className += " " + cls;
    }

    function removeClass(ele, cls) {
        if (hasClass(ele, cls)) {
            var reg = new RegExp('(\\s|^)' + cls + '(\\s|$)','ig');
            ele.className = ele.className.replace(reg, ' ');
        }
    }

</script>
