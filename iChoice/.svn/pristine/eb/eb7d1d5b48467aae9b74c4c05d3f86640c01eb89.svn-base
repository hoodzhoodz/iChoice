<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    xmlns:custom="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:fitsSystemWindows="true"
    android:layout_height="match_parent">

    <FrameLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5">

                <com.choicemmed.ichoice.healthcheck.view.EcgScaleViewSplit
                    android:id="@+id/home_vEcgScaleView1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    custom:bigCellCount="10" />

                <com.choicemmed.ichoice.healthcheck.view.EcgViewSplit
                    android:id="@+id/home_vEcgBarView1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    custom:bigCellCount="10"
                    custom:dataCount="7500" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5">

                <com.choicemmed.ichoice.healthcheck.view.EcgScaleViewSplit
                    android:id="@+id/home_vEcgScaleView2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    custom:bigCellCount="10" />

                <com.choicemmed.ichoice.healthcheck.view.EcgViewSplit
                    android:id="@+id/home_vEcgBarView2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    custom:bigCellCount="10"
                    custom:dataCount="7500" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5">

                <com.choicemmed.ichoice.healthcheck.view.EcgScaleViewSplit
                    android:id="@+id/home_vEcgScaleView3"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    custom:bigCellCount="10" />

                <com.choicemmed.ichoice.healthcheck.view.EcgViewSplit
                    android:id="@+id/home_vEcgBarView3"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    custom:bigCellCount="10"
                    custom:dataCount="7500" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="top|right"
            android:layout_marginRight="5dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:textColor="@color/black"
                android:text="10mm/mV" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:textColor="@color/black"
                android:text="  12.5mm/s  0.5-40Hz" />
        </LinearLayout>

        <Button
            android:id="@+id/home_imb_amplify"
            android:background="@drawable/bg_unchoice_black"
            android:textColor="@color/app_back"
            android:text="@string/app_back"
            android:textAllCaps="false"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:textSize="20sp"
            android:layout_gravity="top|left"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="6dp" />
    </FrameLayout>
</LinearLayout>