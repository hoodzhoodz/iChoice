public class TemperatureUtils {


    /**
     * 摄氏温度
     */
    public static final int Temperature_Celsius=0;

    /**
     * 华氏温度
     */
    public static final int Temperature_Fahrenheit=1;


    /**
     * 摄氏温度转华氏温度
     *
     * @param value
     * @return
     */
    public  static float celsius2Fahrenheit(float value){
        return (float) (1.8 * value + 32);
    }

}
