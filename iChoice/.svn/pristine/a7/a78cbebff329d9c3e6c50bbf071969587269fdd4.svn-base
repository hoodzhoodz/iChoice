package com.choicemmed.ichoice.healthcheck.entity;

import android.support.annotation.NonNull;

public class AvgOxData {
    private int SpO2Avg = 0;
    private int PRAvg = 0;
    private int RRAvg = 0;

    public AvgOxData(int spo2Avg, int prAvg, int rrAvg) {
        SpO2Avg = spo2Avg;
        PRAvg = prAvg;
        RRAvg = rrAvg;
    }

    public void setSpO2Avg(int spO2Avg) {
        SpO2Avg = spO2Avg;
    }

    public void setPRAvg(int PRAvg) {
        this.PRAvg = PRAvg;
    }

    public void setRRAvg(int RRAvg) {
        this.RRAvg = RRAvg;
    }

    public int getSpO2Avg() {
        return SpO2Avg;
    }

    public int getPRAvg() {
        return PRAvg;
    }

    public int getRRAvg() {
        return RRAvg;
    }

    @NonNull
    @Override
    public String toString() {
        return "AvgOxData{" + "SpO2Avg=" + SpO2Avg + ",PRAvg=" + PRAvg + ",RRAvg=" + RRAvg + "}";
    }
}
