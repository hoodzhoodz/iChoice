public class ErrorCode {
    /**
     * errorCode 连接超时
     */
    public static final int ERROR_CONNECT_TIMEOUT = 0x01;
    /**
     * errorCode gatt 异常
     */
    public static final int ERROR_GATT_EXCEPTION = 0x02;
    /**
     * errorCode 蓝牙连接断开
     */
    public static final int ERROR_BLE_DISCONNECT = 0x03;
    /**
     * errorCode 扫描超时
     */
    public static final int ERROR_SCAN_TIMEOUT = 0x04;
    /**
     * errorCode 设备不支持蓝牙
     */
    public static final int ERROR_BLUETOOTH_NOT_SUPPORTED = 0x05;
    /**
     * errorCode 设备不支持蓝牙低功耗技术
     */
    public static final int ERROR_BLE_NOT_SUPPORTED = 0x06;

    /**
     * errorCode 请检查设备的蓝牙开关是否打开
     */
    public static final int ERROR_BLUETOOTH_NOT_OPEN = 0x07;
    /**
     * errorCode 开始发现服务失败
     */
    public static final int ERROR_FOUND_SERVICE_FAIL = 0x08;
    /**
     * errorCode 监听notify特征失败
     */
    public static final int ERROR_CHARACTERISTIC_NOTIFICATION_FAIL = 0x09;
    /**
     * errorCode 写描述失败
     */
    public static final int ERROR_WRITE_DESCRIPTOR_FAIL = 0x0a;
    /**
     * errorCode 写特征失败
     */
    public static final int ERROR_WRITE_CHARACTERISTIC_FAIL = 0x0b;
}
