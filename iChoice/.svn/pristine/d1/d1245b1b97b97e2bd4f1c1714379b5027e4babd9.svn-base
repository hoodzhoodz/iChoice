<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/rl_topbar"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:background="@color/colorPrimary">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="160dp"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.2"></RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1.2">

                        <TextView
                            android:id="@+id/tv_sex"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="34dp"
                            android:text="Female"
                            android:textColor="@color/color_ffffff"
                            android:textSize="13sp"></TextView>

                        <TextView
                            android:id="@+id/tv_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="37dp"
                            android:text="2020-08-12 18:00:12"
                            android:textColor="@color/color_ffffff"
                            android:textSize="10sp"
                            android:textStyle="bold"></TextView>

                        <TextView
                            android:id="@+id/tv_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="34dp"
                            android:text="liyang"
                            android:textColor="@color/color_ffffff"
                            android:textSize="20sp"></TextView>
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Height(cm)"
                                android:textSize="12sp"
                                android:textColor="@color/color_41"
                                android:layout_marginLeft="34dp"
                                android:layout_centerVertical="true"></TextView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Weight(kg)"
                                android:textSize="12sp"
                                android:textColor="@color/color_41"
                                android:layout_centerInParent="true"></TextView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Duration(s)"
                                android:textSize="12sp"
                                android:textColor="@color/color_41"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="34dp"
                                android:layout_centerVertical="true"></TextView>

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="2">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="170"
                                android:textSize="18sp"
                                android:textColor="@color/color_ffffff"
                                android:layout_marginLeft="34dp"
                                android:id="@+id/tv_height"></TextView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="62"
                                android:textSize="18sp"
                                android:textColor="@color/color_ffffff"
                                android:layout_centerHorizontal="true"
                                android:id="@+id/tv_weight"></TextView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="30"
                                android:textSize="18sp"
                                android:textColor="@color/color_ffffff"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="60dp"
                                android:id="@+id/tv_duration"></TextView>




                        </RelativeLayout>


                    </LinearLayout>


                </LinearLayout>
            </RelativeLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="160dp">

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    app:cardCornerRadius="15dp">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:gravity="center"
                            android:text="@string/ecg_analysis_results"
                            android:textColor="@color/color_000000"
                            android:textSize="18sp">

                        </TextView>

                        <LinearLayout
                            android:id="@+id/ll_fibrillation"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="34dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="20dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/atrial_fibrillation"
                                android:textColor="@color/color_000000"
                                android:textSize="12sp" />

                            <TextView
                                android:id="@+id/tv_atrial_fibrillation"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:gravity="start"
                                android:textColor="@color/color_000000"
                                android:textSize="12sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_premature"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="34dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginEnd="20dp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/premature_beat"
                                    android:textColor="@color/color_000000"
                                    android:textSize="12sp" />

                                <TextView
                                    android:id="@+id/tv_premature1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="start"
                                    android:textColor="@color/color_000000"
                                    android:textSize="12sp"
                                    android:visibility="gone" />
                            </LinearLayout>


                            <TextView
                                android:id="@+id/tv_premature"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:gravity="start"
                                android:textColor="@color/color_000000"
                                android:textSize="12sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_bradycardia"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="34dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginEnd="20dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/bradycardia"
                                android:textColor="@color/color_000000"
                                android:textSize="12sp" />

                            <TextView
                                android:id="@+id/tv_bradycardia"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:gravity="start"
                                android:textColor="@color/color_000000"
                                android:textSize="12sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="34dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginEnd="20dp"
                            android:orientation="horizontal"
                            android:id="@+id/ll_tach">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/Tachycardia"
                                android:textColor="@color/color_000000"
                                android:textSize="12sp" />

                            <TextView
                                android:id="@+id/tv_tachycardia"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:gravity="start"
                                android:textColor="@color/color_000000"
                                android:textSize="12sp" />

                        </LinearLayout>

                        <TextView
                            android:id="@+id/txv_other"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="34dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="15dp"
                            android:text="@string/no_e"
                            android:textColor="@color/color_000000"
                            android:textSize="12sp" />

                    </LinearLayout>

                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    app:cardCornerRadius="15dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            android:gravity="center"
                            android:text="@string/ecg_analysis_results_details"
                            android:textColor="@color/color_000000"
                            android:textSize="18sp"></TextView>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:gravity="center"
                            android:text="@string/ecg_analysis_heart_rate_indicator"
                            android:textColor="@color/color_000000"
                            android:textSize="15sp"></TextView>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="31dp">

                            <TextView
                                android:id="@+id/avg_hr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="34dp"
                                android:text="Avg HR"
                                android:textColor="@color/color_000000"
                                android:textSize="10sp"></TextView>


                            <TextView
                                android:id="@+id/tv_avg_hr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/avg_hr"
                                android:layout_marginLeft="34dp"
                                android:layout_marginTop="8dp"
                                android:text="62"
                                android:textColor="@color/color_098bfa"
                                android:textSize="18sp"></TextView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignBaseline="@id/tv_avg_hr"
                                android:layout_marginLeft="6dp"
                                android:layout_toRightOf="@id/tv_avg_hr"
                                android:text="bpm"
                                android:textColor="@color/color_b5b5b5"
                                android:textSize="10sp"></TextView>


                            <TextView
                                android:id="@+id/slowest_hr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:text="Slowest HR"
                                android:textColor="@color/color_000000"
                                android:textSize="10sp"
                                android:paddingLeft="16dp"></TextView>

                            <TextView
                                android:id="@+id/tv_slow_hr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_below="@id/slowest_hr"


                                android:layout_marginTop="8dp"
                                android:text="70"
                                android:textColor="@color/color_098bfa"
                                android:textSize="18sp"></TextView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/slowest_hr"
                                android:text="bpm"
                                android:textColor="@color/color_b5b5b5"
                                android:textSize="10sp"
                                android:layout_toRightOf="@id/tv_slow_hr"
                                android:layout_alignBaseline="@id/tv_slow_hr"
                                android:layout_marginLeft="6dp"></TextView>


                            <TextView
                                android:id="@+id/fast_hr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="34dp"
                                android:text="Fastest HR"
                                android:textColor="@color/color_000000"
                                android:textSize="10sp"></TextView>

                            <TextView
                                android:id="@+id/tv2"
                                android:layout_alignParentRight="true"
                                android:layout_below="@id/avg_hr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignBaseline="@id/tv_fast_hr"
                                android:text="bpm"
                                android:textColor="@color/color_b5b5b5"
                                android:textSize="10sp"
                                android:layout_marginRight="34dp"></TextView>

                            <TextView
                                android:id="@+id/tv_fast_hr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/avg_hr"
                                android:layout_toLeftOf="@id/tv2"
                                android:layout_marginTop="8dp"
                                android:text="72"
                                android:textColor="@color/color_098bfa"
                                android:textSize="18sp"
                                android:layout_marginRight="6dp"></TextView>

                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="27dp">

                            <TextView
                                android:id="@+id/avg_rr_interval"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="34dp"
                                android:text="Avg HR interval"
                                android:textColor="@color/color_000000"
                                android:textSize="10sp"></TextView>


                            <TextView
                                android:id="@+id/tv_avg_rr_interval"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/avg_rr_interval"
                                android:layout_marginLeft="34dp"
                                android:layout_marginTop="8dp"
                                android:text="680"
                                android:textColor="@color/color_098bfa"
                                android:textSize="18sp"></TextView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignBaseline="@id/tv_avg_rr_interval"
                                android:layout_marginLeft="6dp"
                                android:layout_toRightOf="@id/tv_avg_rr_interval"
                                android:text="ms"
                                android:textColor="@color/color_b5b5b5"
                                android:textSize="10sp"></TextView>


                            <TextView
                                android:id="@+id/max_rr_interval"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:text="Max RR Interval"
                                android:textColor="@color/color_000000"
                                android:textSize="10sp"
                                android:paddingLeft="16dp"></TextView>

                            <TextView
                                android:id="@+id/tv_max_rr_interval"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_below="@id/max_rr_interval"
                                android:layout_marginTop="8dp"
                                android:text="712"
                                android:textColor="@color/color_098bfa"
                                android:textSize="18sp"></TextView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/max_rr_interval"
                                android:text="ms"
                                android:textColor="@color/color_b5b5b5"
                                android:textSize="10sp"
                                android:layout_toRightOf="@id/tv_max_rr_interval"
                                android:layout_alignBaseline="@id/tv_max_rr_interval"
                                android:layout_marginLeft="6dp"></TextView>


                            <TextView
                                android:id="@+id/aix_rr_interval"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="34dp"
                                android:text="Min RR Interval"
                                android:textColor="@color/color_000000"
                                android:textSize="10sp"></TextView>

                            <TextView
                                android:id="@+id/tv3"
                                android:layout_alignParentRight="true"
                                android:layout_below="@id/aix_rr_interval"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignBaseline="@id/tv_aix_rr_interval"
                                android:text="ms"
                                android:textColor="@color/color_b5b5b5"
                                android:textSize="10sp"
                                android:layout_marginRight="34dp"></TextView>

                            <TextView
                                android:id="@+id/tv_aix_rr_interval"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/aix_rr_interval"
                                android:layout_toLeftOf="@id/tv3"
                                android:layout_marginTop="8dp"
                                android:text="630"
                                android:textColor="@color/color_098bfa"
                                android:textSize="18sp"
                                android:layout_marginRight="6dp"></TextView>

                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="27dp">


                            <TextView
                                android:id="@+id/sdnn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="34dp"
                                android:text="SDNN"
                                android:textColor="@color/color_000000"
                                android:textSize="10sp"></TextView>


                            <TextView
                                android:id="@+id/tv_sdnn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/sdnn"
                                android:layout_marginLeft="34dp"
                                android:layout_marginTop="8dp"
                                android:text="680"
                                android:textColor="@color/color_098bfa"
                                android:textSize="18sp"></TextView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignBaseline="@id/tv_sdnn"
                                android:layout_marginLeft="6dp"
                                android:layout_toRightOf="@id/tv_sdnn"
                                android:text="ms"
                                android:textColor="@color/color_b5b5b5"
                                android:textSize="10sp"></TextView>


                        </RelativeLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="50dp"
                            android:gravity="center"
                            android:text="@string/ecg_arrhythmia_indicator"
                            android:textColor="@color/color_000000"
                            android:textSize="15sp"></TextView>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="30dp"
                            android:layout_marginBottom="26dp">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_marginLeft="34dp">

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:id="@+id/ll_afib">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"

                                        android:orientation="horizontal">

                                        <TextView
                                            android:layout_height="match_parent"
                                            android:layout_width="wrap_content"
                                            android:drawableRight="@mipmap/ecg_heart"></TextView>

                                        <TextView
                                            android:layout_marginLeft="4dp"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:textSize="10sp"
                                            android:text="Afib"
                                            android:textColor="@color/color_000000"
                                            android:id="@+id/afib"
                                            android:gravity="center"></TextView>

                                        <TextView
                                            android:layout_height="match_parent"
                                            android:layout_width="wrap_content"
                                            android:drawableLeft="@mipmap/ecg_sanjiao"
                                            android:layout_marginLeft="8dp"></TextView>
                                    </LinearLayout>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/tv_afib"
                                        android:textSize="18sp"
                                        android:textColor="@color/color_098bfa"></TextView>
                                </LinearLayout>


                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:layout_marginTop="20dp"
                                    android:id="@+id/bradycardia">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"

                                        android:orientation="horizontal">

                                        <TextView
                                            android:layout_height="match_parent"
                                            android:layout_width="wrap_content"
                                            android:drawableRight="@mipmap/ecg_heart"></TextView>

                                        <TextView
                                            android:layout_marginLeft="4dp"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:textSize="10sp"
                                            android:text="Bradycardia"
                                            android:textColor="@color/color_000000"
                                            android:gravity="center"></TextView>

                                        <TextView
                                            android:layout_height="match_parent"
                                            android:layout_width="wrap_content"
                                            android:drawableLeft="@mipmap/ecg_sanjiao"
                                            android:layout_marginLeft="8dp"></TextView>
                                    </LinearLayout>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/tv_bradycardia1"
                                        android:text="No"
                                        android:textColor="@color/color_098bfa"
                                        android:textSize="18sp"></TextView>
                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="34dp">

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"

                                    android:id="@+id/premature_beats"

                                    >


                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"

                                        >

                                        <TextView
                                            android:layout_height="match_parent"
                                            android:layout_width="wrap_content"
                                            android:drawableRight="@mipmap/ecg_heart"></TextView>

                                        <TextView
                                            android:layout_marginLeft="4dp"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:textSize="10sp"
                                            android:text="Premature beats"
                                            android:textColor="@color/color_000000"
                                            android:gravity="center"></TextView>

                                        <TextView
                                            android:layout_height="match_parent"
                                            android:layout_width="wrap_content"
                                            android:drawableLeft="@mipmap/ecg_sanjiao"
                                            android:layout_marginLeft="8dp"></TextView>
                                    </LinearLayout>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/tv_beats"
                                        android:text="No"
                                        android:textColor="@color/color_098bfa"
                                        android:textSize="18sp"></TextView>

                                </LinearLayout>


                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:layout_marginTop="20dp"
                                    android:id="@+id/ll_tachycardia">


                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"

                                        >

                                        <TextView
                                            android:layout_height="match_parent"
                                            android:layout_width="wrap_content"
                                            android:drawableRight="@mipmap/ecg_heart"></TextView>

                                        <TextView
                                            android:layout_marginLeft="4dp"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:textSize="10sp"
                                            android:text="Tachycardia"
                                            android:textColor="@color/color_000000"
                                            android:gravity="center"></TextView>

                                        <TextView
                                            android:layout_height="match_parent"
                                            android:layout_width="wrap_content"
                                            android:drawableLeft="@mipmap/ecg_sanjiao"
                                            android:layout_marginLeft="8dp"></TextView>
                                    </LinearLayout>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/tv_tachycardia1"
                                        android:text="No"
                                        android:textColor="@color/color_098bfa"
                                        android:textSize="18sp"></TextView>

                                </LinearLayout>
                            </LinearLayout>


                        </RelativeLayout>


                    </LinearLayout>

                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginBottom="20dp"
                    app:cardCornerRadius="15dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="11dp"
                            android:layout_marginBottom="13dp"
                            android:layout_marginLeft="34dp"
                            android:gravity="center_vertical">

                            <TextView
                                android:layout_height="match_parent"
                                android:layout_width="wrap_content"
                                android:drawableRight="@mipmap/ecg_heart"
                                android:id="@+id/tv_11"
                                android:layout_centerVertical="true"></TextView>

                            <TextView
                                android:layout_centerVertical="true"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:textSize="10sp"
                                android:text="Avg.HR"
                                android:textColor="@color/color_b5b5b5"
                                android:gravity="center"
                                android:layout_toRightOf="@id/tv_11"
                                android:layout_marginLeft="8dp"
                                android:id="@+id/tv_22"></TextView>

                            <TextView
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="8dp"
                                android:layout_toRightOf="@id/tv_22"
                                android:id="@+id/tv_avg_hr1"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:text="67"
                                android:textColor="@color/color_098bfa"
                                android:textSize="12sp"></TextView>

                            <TextView
                                android:layout_centerVertical="true"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignBaseline="@id/tv_avg_hr1"
                                android:layout_marginLeft="8dp"
                                android:layout_toRightOf="@id/tv_avg_hr1"
                                android:text="bpm"
                                android:textColor="@color/color_b5b5b5"
                                android:textSize="13sp"></TextView>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:orientation="horizontal"
                                android:layout_centerVertical="true"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="34dp"
                                android:id="@+id/ll_full_ecg">

                                <TextView
                                    android:layout_centerVertical="true"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:textSize="10sp"
                                    android:text="View full ECG"
                                    android:textColor="@color/color_b5b5b5"
                                    android:gravity="center"
                                    android:layout_toLeftOf="@id/tv_full_ecg"
                                    android:layout_marginRight="11dp"></TextView>

                                <TextView

                                    android:layout_height="match_parent"
                                    android:layout_width="wrap_content"
                                    android:drawableLeft="@mipmap/ecg_sanjiao"
                                    android:id="@+id/tv_full_ecg"></TextView>
                            </LinearLayout>


                        </RelativeLayout>


                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <com.choicemmed.ichoice.healthcheck.view.EcgView
                                android:id="@+id/home_vEcgBarView"
